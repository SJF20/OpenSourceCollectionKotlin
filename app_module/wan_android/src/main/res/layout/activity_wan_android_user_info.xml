<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <import type="com.shijingfeng.wan_android.view_model.UserInfoViewModel" />

        <variable
            name="userInfoViewModel"
            type="UserInfoViewModel" />
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include
            android:id="@+id/include_title_bar"
            layout="@layout/layout_wan_android_title_bar"
            app:ivBackClick="@{userInfoViewModel.mBackClickListener}" />

    </LinearLayout>

</layout>