    <style name="GlobalActivityAnimation" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/anim_activity_open_enter</item>
        <item name="android:activityOpenExitAnimation">@anim/anim_activity_open_exit</item>
        <item name="android:activityCloseEnterAnimation">@anim/anim_activity_close_enter</item>
        <item name="android:activityCloseExitAnimation">@anim/anim_activity_close_exit</item>
    </style>
    <style name="ProjectTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@style/GlobalActivityAnimation</item>
    </style>
    <style name="ProjectTheme.Launcher">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowDisablePreview">true</item>
    </style>
    <style name="bottom_dialog_anim" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/anim_bottom_dialog_show</item>
        <item name="android:windowExitAnimation">@anim/anim_bottom_dialog_hide</item>
    </style>
    <style name="center_dialog_anim" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/anim_center_dialog_show</item>
        <item name="android:windowExitAnimation">@anim/anim_center_dialog_hide</item>
    </style>
    <style name="top_dialog_anim" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/anim_top_dialog_show</item>
        <item name="android:windowExitAnimation">@anim/anim_top_dialog_hide</item>
